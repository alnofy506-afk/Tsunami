local function pullHeart(touch)
    local player = game.Players.LocalPlayer
    local character = player.Character
    local root = character and character:FindFirstChild("HumanoidRootPart")
    if not root then return end
    local item = touch.Parent
    if item and item:IsA("BasePart") then
        local name = item.Name:lower()
        if name:find("heart") or (item.Parent and item.Parent.Name:lower():find("heart")) then
            firetouchinterest(root, item, 0)
            task.wait()
            firetouchinterest(root, item, 1)
        end
    end
end

local function pullArcadeStuff(touch)
    local player = game.Players.LocalPlayer
    local character = player.Character
    local root = character and character:FindFirstChild("HumanoidRootPart")
    if not root then return end
    local item = touch.Parent
    if item and item:IsA("BasePart") then
        local name = item.Name:lower()
        if name:find("ticket") or name:find("arcade") or name:find("controller") or name:find("remote") or name:find("joystick") then
            firetouchinterest(root, item, 0)
            task.wait()
            firetouchinterest(root, item, 1)
        end
    end
end

-- ÙˆØ¸ÙŠÙØ© Ø¬Ù„Ø¨ Ø§Ù„Ù€ Plot ID
local function getMyPlotID()
    local player = game.Players.LocalPlayer
    local foundID = nil
    pcall(function()
        for _, plot in pairs(workspace:WaitForChild("Plots"):GetChildren()) do
            if plot:FindFirstChild("Owner") and plot.Owner.Value == player then
                foundID = plot.Name
            end
        end
    end)
    return foundID or "{02431b3a-5fcf-40e4-90aa-220317656166}"
end

